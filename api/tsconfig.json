{
  "compilerOptions": {
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "target": "es6",
      "noImplicitAny": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "src/routes/dist",
      "baseUrl": "src",
      "paths": {
      "*": ["node_modules/*", "src/types/*"]
      },
      "resolveJsonModule": true
  },
  "include": ["src/**/*"]
}